<Window x:Class="t2.Ad1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:t2"
        mc:Ignorable="d"
        Title="Ad1" WindowState="Maximized" Width="1537.6" Height="864.8">
    <Grid>
        <MediaElement Name="ad1" Source="C:/Users/user/Desktop/ad1.mp4" LoadedBehavior="Manual" UnloadedBehavior="Stop" 
     MediaOpened="Element_MediaOpened" MediaEnded="Element_MediaEnded"/>
        <StackPanel>
            <Slider Name="timelineSlider" Margin="5" Width="70" Opacity="0">
                <Slider.Background>
                    <SolidColorBrush Color="Transparent" Opacity="0"/>
                </Slider.Background>
            </Slider>
        </StackPanel>
    </Grid>

    
</Window>
